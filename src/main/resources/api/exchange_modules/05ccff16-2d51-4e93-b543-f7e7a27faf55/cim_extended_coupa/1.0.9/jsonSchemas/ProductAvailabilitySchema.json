{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/ProductAvailability",
  "definitions": {
    "ProductAvailability": {
      "description": "Availability of product for location",
      "type": "object",
      "example": {
        "id": "pariatur tempor occaecat adipisicing",
        "variantId": "deserunt dolor est pariatur officia",
        "locationId": "aliquip dolor Lorem",
        "locationType": "WAREHOUSE",
        "quantityInStock": 5590,
        "locationName": "ullamco proident consectetur anim",
        "name": "labore",
        "externalId": "culpa"
      },
      "additionalProperties": true,
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "description": "Identifier of product"
        },
        "name": {
          "type": "string",
          "description": "Name of the product"
        },
        "externalId": {
          "type": "string",
          "description": "Id in external system"
        },
        "variantId": {
          "type": "string",
          "description": "Identifier of product variant"
        },
        "locationId": {
          "type": "string",
          "description": "Identifier of location"
        },
        "locationName": {
          "type": "string",
          "description": "Identifier of location"
        },
        "locationType": {
          "type": "string",
          "enum": [
            "STORE",
            "WAREHOUSE",
            "PARTNER"
          ],
          "description": "Type of location"
        },
        "quantityInStock": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "description": "Count of products in stock"
        }
      }
    }
  }
}
