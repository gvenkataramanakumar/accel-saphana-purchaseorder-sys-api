{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/ProductAvailabilityDetail",
  "definitions": {
    "ProductAvailabilityDetail": {
      "description": "Represents the availability of product for location",
      "type": "object",
      "example": {
        "id": "pariatur tempor occaecat adipisicing",
        "variantId": "deserunt dolor est pariatur officia",
        "locationId": "aliquip dolor Lorem",
        "locationType": "WAREHOUSE",
        "quantityInStock": 5590,
        "locationName": "ullamco proident consectetur anim",
        "name": "labore",
        "externalId": "culpa"
      },
      "additionalProperties": true,
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "description": "Identifier of product"
        },
        "name": {
          "type": "string",
          "description": "Name of the product"
        },
        "externalId": {
          "type": "string",
          "description": "Id in external system"
        },
        "variantId": {
          "type": "string",
          "description": "Identifier of product variant"
        },
        "locationId": {
          "type": "string",
          "description": "Identifier of location"
        },
        "locationName": {
          "type": "string",
          "description": "Identifier of location"
        },
        "locationType": {
          "type": "string",
          "enum": [
            "STORE",
            "WAREHOUSE",
            "PARTNER"
          ],
          "description": "Type of location"
        },
        "auditDetail": {
          "description": "Defines standard system audit fields available for most types",
          "example": {
            "createdOn": "2021-01-21T21:21:21Z",
            "createdBy": "0054x000002IpBlAAK",
            "updatedOn": "2021-01-21T21:21:21Z",
            "updatedBy": "0054x000002xBGGAA2",
            "isDeleted": false
          },
          "type": "object",
          "additionalProperties": true,
          "required": [
            "createdOn",
            "createdBy",
            "updatedOn",
            "updatedBy",
            "isDeleted"
          ],
          "properties": {
            "createdOn": {
              "type": "string",
              "format": "date-time"
            },
            "createdBy": {
              "type": "string"
            },
            "updatedOn": {
              "type": "string",
              "format": "date-time"
            },
            "updatedBy": {
              "type": "string"
            },
            "isDeleted": {
              "type": "boolean"
            }
          }
        },
        "quantityInStock": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "description": "Count of products in stock"
        }
      }
    }
  }
}
