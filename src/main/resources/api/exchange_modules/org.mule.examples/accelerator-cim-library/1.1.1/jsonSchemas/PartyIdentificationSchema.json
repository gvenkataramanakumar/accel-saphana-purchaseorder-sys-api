{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/PartyIdentification",
  "definitions": {
    "PartyIdentification": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "name",
        "id",
        "identificationNumber",
        "partyIdentificationType"
      ],
      "properties": {
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "name": {
          "type": "string"
        },
        "expiryDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "null"
            }
          ]
        },
        "partyIdentificationType": {
          "type": "string"
        },
        "verifiedDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "null"
            }
          ]
        },
        "issuedByAuthority": {
          "type": "string"
        },
        "issuedAtLocation": {
          "type": "string"
        },
        "issuedDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "null"
            }
          ]
        },
        "partyRole": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "identificationNumber": {
          "type": "string"
        },
        "party": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
