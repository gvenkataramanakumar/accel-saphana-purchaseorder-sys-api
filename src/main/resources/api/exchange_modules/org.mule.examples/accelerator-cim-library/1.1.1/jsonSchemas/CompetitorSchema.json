{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Competitor",
  "definitions": {
    "Competitor": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "competitorFromDate",
        "id"
      ],
      "properties": {
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "competitorStrengths": {
          "type": "string"
        },
        "competitorFromDate": {
          "type": "string",
          "format": "date"
        },
        "socialMediaPostRate": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "competitorThreats": {
          "type": "string"
        },
        "currentRatio": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "cashFlowGrowth": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "competitorObjectives": {
          "type": "string"
        },
        "netProfitMargin": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "competitorWeaknesses": {
          "type": "string"
        },
        "pressMentionsRate": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "assetTurnoverRatio": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "competitorToDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "null"
            }
          ]
        },
        "competitorLeverageOpportunities": {
          "type": "string"
        },
        "assetReturnRatio": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "ptoENumber": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "adKeywords": {
          "type": "string"
        },
        "adReachRate": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "party": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
