{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/UncategorizedParty",
  "definitions": {
    "UncategorizedParty": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "partyType",
        "uncategorizedPartyLabel",
        "id"
      ],
      "properties": {
        "partyType": {
          "type": "string"
        },
        "globalParty": {
          "type": "string"
        },
        "noMergeReason": {
          "type": "string"
        },
        "primaryAccount": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "creationReason": {
          "type": "string"
        },
        "uncategorizedPartyLabel": {
          "type": "string"
        },
        "originalSourceRecord": {
          "type": "string"
        },
        "originalSourceSystem": {
          "type": "string"
        }
      }
    }
  }
}
