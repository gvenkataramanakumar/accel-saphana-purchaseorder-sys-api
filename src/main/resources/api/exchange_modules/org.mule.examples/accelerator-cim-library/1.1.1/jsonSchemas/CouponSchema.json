{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Coupon",
  "definitions": {
    "Coupon": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "id",
        "paymentMethodStatus"
      ],
      "properties": {
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "manufacturer": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "primaryScanCodeLabel": {
          "type": "string"
        },
        "isReturnCoupon": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ]
        },
        "expirationDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "null"
            }
          ]
        },
        "secondaryScanCodeLabel": {
          "type": "string"
        },
        "scanCode": {
          "type": "string"
        },
        "couponCode": {
          "type": "string"
        },
        "returnCouponReason": {
          "type": "string"
        },
        "discountAmount": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "isValid": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ]
        },
        "couponStatus": {
          "type": "string"
        },
        "discountPercentage": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "couponCount": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "paymentMethodType": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "paymentMethodStatus": {
          "type": "string"
        }
      }
    }
  }
}
