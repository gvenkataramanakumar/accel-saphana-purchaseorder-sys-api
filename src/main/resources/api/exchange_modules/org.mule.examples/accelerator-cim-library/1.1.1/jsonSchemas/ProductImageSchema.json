{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/ProductImage",
  "definitions": {
    "ProductImage": {
      "type": "object",
      "example": {
        "id": "id1",
        "product": [
          "nisi",
          "deserunt"
        ],
        "isDefaultImage": false,
        "imageViewType": "eiusmod adipisicing commodo et"
      },
      "additionalProperties": true,
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "product": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "isDefaultImage": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ]
        },
        "imageViewType": {
          "type": "string"
        }
      }
    }
  }
}
